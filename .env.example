# ShareConnect Signing Configuration
# Copy this file to .env.properties and fill in your actual values
# These values can also be set as system environment variables

# Development signing configuration
SHARECONNECT_DEV_KEY_ALIAS=your_dev_key_alias
SHARECONNECT_DEV_KEY_PASSWORD=your_dev_key_password
SHARECONNECT_DEV_STORE_PASSWORD=your_dev_store_password
SHARECONNECT_DEV_KEYSTORE_PATH=Signing/dev.jks

# Cloud/Production signing configuration
SHARECONNECT_CLOUD_KEY_ALIAS=your_cloud_key_alias
SHARECONNECT_CLOUD_KEY_PASSWORD=your_cloud_key_password
SHARECONNECT_CLOUD_STORE_PASSWORD=your_cloud_store_password
SHARECONNECT_CLOUD_KEYSTORE_PATH=Signing/cloud.jks

# Note: Priority order for configuration values:
# 1. .env.properties file (if exists)
# 2. System environment variables
# 3. Default values in build.gradle

FIREBASE_TOKEN=""
FIREBASE_DISTRIBUTION_PROD_GROUP=""
FIREBASE_DISTRIBUTION_PROD_ARTIFACT_TYPE="APK"
FIREBASE_DISTRIBUTION_PROD_APP_ID=""
FIREBASE_DISTRIBUTION_PROD_APP_CREDENTIALS_FILE=""
